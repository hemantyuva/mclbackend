<span><%= answer.votes.count %></span>
<% if answer.vote_present?(@associate_user) %>
	<%= link_to destroy_answer_votes_answer_vote_path(answer,owned_user_like(answer,@associate_user)), :method=>"delete", :remote=>true do %>
	<span class="glyphicon glyphicon-thumbs-up"></span>
	<%end%>
<% else %>
	<%= link_to create_answer_votes_answer_votes_path(answer.id), :method=>"post", :remote=>true do %>
		<span class="glyphicon glyphicon-thumbs-up" style="color:#333333;"></span>
	<%end%>
<% end %>