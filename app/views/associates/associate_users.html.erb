<div class="row">
<div class="col-md-12 col-sm-12 col-xs-12">
  <div class="top-header">
    <div class="col-sm-3 col-xs-3 col-md-3">
      <h4>          
        <%= link_to settings_path do %>
          <i class="fa fa-chevron-left cse-shw-dte shw-page-updte">Settings</i>
        <%end%>
      </h4>
    </div>
    <div class="col-sm-6 col-xs-6 col-md-6">
      <h4 class="text-center">Accepted invitations</h4>
    </div>
    <div class="col-sm-3 col-xs-3 col-md-3">
      <h4></h4>
    </div>
  </div>
</div>
</div><br>
<div class="clearfix"></div>
<div class="invitees-top"></div>
<div class="invitees-list">
  <%@associates.each do |associate|%>
    <div class="profile-setting">
      <div class="col-md-2">
        <div class="img-1">
          <%= link_to root_path(surgeon_id: associate.invitee.id,associate_id: associate.id) do %>
            <%= user_image associate.invitee%>
          <% end %>
        </div>
      </div>
      <div class="col-md-10">
        <%= link_to root_path(surgeon_id: associate.invitee.id,associate_id: associate.id) do %>
            <%=associate.invitee.name%>
          <% end %>
      </div>
    </div>
      <div class="clearfix"></div>
  <%end%>
</div>