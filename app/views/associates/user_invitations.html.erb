<div class="row">
<div class="col-md-12 col-sm-12 col-xs-12">
  <div class="top-header">
    <div class="col-sm-3 col-xs-3 col-md-3">
      <h4>          
        <%= link_to settings_path do %>
          <i class="fa fa-chevron-left cse-shw-dte shw-page-updte">Settings</i>
        <%end%>
      </h4>
    </div>
    <div class="col-sm-6 col-xs-6 col-md-6">
      <h4 class="text-center">List of invitees</h4>
    </div>
    <div class="col-sm-3 col-xs-3 col-md-3">
      <h4></h4>
    </div>
  </div>
</div>
</div><br>
<div class="clearfix"></div>
<div class="invitees-top"></div>
<div class="invitees-list">
	<%@invitations.each do |invitation|%>
		<div class="col-md-8">
      <%=invitation.invitee.name%>
    </div>
    <div class="col-md-2">
        <%=link_to "Accept",accept_associate_path(invitation.id)%>
    </div>
    <div class="col-md-2">
      <%=link_to "Reject",reject_associate_path(invitation.id)%>
    </div>
	<%end%>
</div>