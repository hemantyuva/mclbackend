<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="top-header">
      <div class="col-sm-3 col-xs-3 col-md-3">
        <h4>          
          <%= link_to associates_path do %>
            <i class="fa fa-chevron-left cse-shw-dte shw-page-updte">Associates</i>
          <%end%>
        </h4>
      </div>
      <div class="col-sm-6 col-xs-6 col-md-6">
        <h4 class="text-center">Add Associate</h4>
      </div>
      <div class="col-sm-3 col-xs-3 col-md-3">
        
      </div>
    </div>
  </div>
</div><br>
<div class="col-md-12 clear-box icon-text-field">
	<i class="fa fa-search search-icon"></i>
		<%= text_field_tag :associate_user_name,nil,placeholder: "Search user by email and mobile number",class: "add-schdle-txt form-control"%>
</div>
<div class="clearfix"></div>
<div id="user_search_list">
</div>
<div class="user-invitation-form">
	<%= form_for @associate, url: associates_path ,:method => "post" do |f|%>
	  <p>
	    <%= f.label :recipient_email, "User email address" %><br />
	    <%= f.text_field :recipient_email, class: "form-control"  %>
	  </p>
	  <p>
	    <%= f.label :recipient_mobile, "User mobile number" %><br />
	    <%= f.text_field :recipient_mobile, class:"form-control"  %>
	  </p>
	  <p><%= f.submit " Send Invite", :class=>"btn btn-sucess send_invite btn-block" %></p>
	<% end %>
</div>
