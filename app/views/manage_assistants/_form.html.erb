<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="top-header">
      <div class="col-sm-3 col-xs-3 col-md-3">
        <h4>          
          <%= link_to manage_assistants_path do %>
            <i class="fa fa-chevron-left cse-shw-dte shw-page-updte">Assistants</i>
          <%end%>
        </h4>
      </div>
      <div class="col-sm-6 col-xs-6 col-md-6">
        <h4 class="text-center">Add Assistant</h4>
      </div>
      <div class="col-sm-3 col-xs-3 col-md-3">
        <h4><%= link_to "Done","#",class: "pull-right submit-assistant-form"%>
      </div>
    </div>
  </div>
</div>
<div class="clearfix"></div>
  <div class="form-horizontal">
    <div class="">
      <div class="col-md-12 loction_frm_clr">
        <%= form_for @assistant,url: create_assistants_manage_assistant_path(@associate_user),:html => { :class => "form-horizontal manage_assistant-form" }  do |assistant|%>
          <div class="form-group">
            <div class="col-sm-10 col-xs-8  col-md-12  clear-box">
              <%= assistant.text_field :assistants, :class => 'form-control',placeholder: "New Assistant Name" %>
            </div>
          </div>
        <%end%>
      </div>
    </div>
  </div>
