<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="top-header">
      <div class="col-sm-3 col-xs-3 col-md-3">
        <h4>          
          <%= link_to anaesthetists_list_manage_assistants_path do %>
            <i class="fa fa-chevron-left cse-shw-dte shw-page-updte">Anaesthetists</i>
          <%end%>
        </h4>
      </div>
      <div class="col-sm-6 col-xs-6 col-md-6">
        <h4 class="text-center">Add Anaesthetist</h4>
      </div>
      <div class="col-sm-3 col-xs-3 col-md-3">
        <h4><%= link_to "Done","#",class: "pull-right submit-anaesthetist-form"%>
      </div>
    </div>
  </div>
</div>
<div class="clearfix"></div>
  <div class="form-horizontal">
    <div class="">
      <div class="col-md-12 loction_frm_clr">
        <%= form_for @anaesthetist,url: create_anaesthetists_manage_assistant_path(@associate_user), method: "post",:html=>{id: "anaesthetist_form"} do |f| %>
          <div class="form-group">
            <div class="col-sm-10 col-xs-8  col-md-12  clear-box">
              <%= f.text_field :anaesthetists,class: "form-control",placeholder: "New Anaesthetist Name"%>
            </div>
          </div>
        <%end%>
      </div>
    </div>
  </div>
