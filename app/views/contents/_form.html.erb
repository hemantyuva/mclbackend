<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="top-header">
      <div class="col-sm-3 col-xs-3 col-md-3">
      </div>
      <div class="col-sm-6 col-xs-6 col-md-6">
      </div>
      <div class="col-sm-3 col-xs-3 col-md-3">
      </div>
    </div>
  </div>
</div>
<%= form_for(@content) do |f| %>
  <% if @content.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@content.errors.count, "error") %> prohibited this speciality from being saved:</h2>

      <ul>
      <% @content.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
      <div id="show_about">
        <div class="col-sm-10 col-xs-8  col-md-10  clear-box">
          <%= f.text_field :about_content, :class => 'form-control' %><br>
          <%= f.submit "Save"%> 
        </div>
      </div>
      <div id="show_faq">
        <div class="col-sm-10 col-xs-8  col-md-10  clear-box">
          <%= f.text_field :faq_content, :class => 'form-control' %><br>
          <%= f.submit "Save"%> 
        </div>
      </div>
  </div>

<% end %>



