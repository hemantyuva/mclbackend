<div class="mange-master">
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Created</th>
      <th>Modified</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @diagnoses.each do |diagnose| %>
      <tr>  
        <td><%= diagnose.name %></td>
        <td><%= spec_date_format(diagnose.created_at)%></td>
        <td><%= spec_date_format(diagnose.updated_at)%></td>
        <td><%= link_to 'Modify',edit_diagnosis_path(diagnose) %></td>
      </tr>
    <% end %>
  </tbody>
</table>