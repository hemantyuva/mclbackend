<div class="row">
  <div class="col-md-12 col-sm-12 col-xs-12">
    <div class="top-header">
      <div class="col-sm-2 col-xs-3 col-md-3">
      </div>
      <div class="col-sm-6 col-xs-6 col-md-6">
        <h4 class="text-center">Dashboard</h4>
      </div>
      <div class="col-sm-2 col-xs-3 col-md-3">  
      </div>
    </div>  
  </div>
  <div class="clearfix"></div>
   <div class="col-md-12 clear-box icon-text-field">
    <div class="col-sm-12 col-xs-12 col-md-12 dashboard-total-fields">
        <div class="col-md-4">
          <div class="col-md-12">Total</div>
          <div class="col-md-12">
            <div class="dash-box1 col-md-12"><h5><b>Users</b></h5>
            <h6 class="text-center"><%= @users.count%></h6></div>
          </div>
        </div>
        <div class="col-md-4">  
          <div class="col-md-12">Total</div>
          <div class="col-md-12">
            <div class="dash-box2 col-md-12">
            <h5><b>Patients</b></h5>
            <h6 class="text-center"><%= @patients.count%></h6></div>
          </div>
        </div>
        <div class="col-md-4">  
          <div class="col-md-12">Total</div>
         <div class="col-md-12">
          <div class="dash-box3 col-md-12"><h5><b>Cases</b></h5>
          <h6 class="text-center"><%= @cases.count%></h6></div>
          </div>
        </div>  
          <div class="clearfix"></div>
          <div class="col-sm-12 col-xs-12 col-md-12">
          <div class="col-md-4 col-xs-8 user-login">
            <h5><b>Currently Logged in users</b></h5>
            <h6 class="text-center"><%= @loggedin_count%></h6>
          </div>
          </div>
    </div>
   </div>
</div>
