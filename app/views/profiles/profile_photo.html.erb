<div class="row">
<div class="col-md-12 col-sm-12 col-xs-12">
  <div class="top-header">
    <div class="col-sm-3 col-xs-3 col-md-3">
      <h4>          
        <%= link_to user_profile_data_profile_path(@associate_user) do %>
          <i class="fa fa-chevron-left cse-shw-dte shw-page-updte">&nbsp;My Profile Data</i>
        <%end%>
      </h4>
    </div>
    <div class="col-sm-6 col-xs-6 col-md-6">
      <h4 class="text-center">My Photos</h4>
    </div>
    <div class="col-sm-3 col-xs-3 col-md-3">
      <h4></h4>
    </div>
  </div>
</div>
</div><br>
<div class="prfle-adres-pading"></div>
  <%= form_for @profile_setting,url: update_profile_photo_profile_path(@associate_user), method: "post" do |f| %>
    <div>
      <%= f.label :profile_image, "Profile Photo"%>
        <%= f.file_field :profile_image %>
    </div>
    <div>
      <%= f.label :cover_image, "Cover Photo"%>
      <%= f.file_field :cover_image %>
    </div><br>
      <%= f.submit "Update",class: "btn btn-success btn"%>
  <%end%>
